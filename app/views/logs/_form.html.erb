<%= form_for(@log) do |f| %>
  <div class="field">
    <%= f.label :type %><br>
    <%= f.text_field :type %>
  </div>
  <div class="field">
    <%= f.label :log %><br>
    <%= f.text_area :log %>
  </div>
  <div class="field">
    <%= f.label :resolve %><br>
    <%= f.text_area :resolve %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
