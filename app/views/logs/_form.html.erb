<%= form_for(@log) do |f| %>
    <%= render 'shared/error_messages', :object => f.object %>
    <div class="form-group">
        <div class="field">
            <%= f.label t(:Type) %><br>
            <%= f.text_field :type, class: 'form-control' %>
        </div>
    </div>
    <div class="form-group">
        <div class="field">
            <%= f.label t(:Log) %><br>
            <%= f.text_area :log, class: 'form-control' %>
        </div>
    </div>
    <div class="form-group">
        <div class="field">
            <%= f.label t(:Url) %><br>
            <%= f.text_field :url, class: 'form-control' %>
        </div>
    </div>
    <div class="form-group">
        <div class="field">
            <%= f.label t(:Resolve) %><br>
            <%= f.text_area :resolve, class: 'form-control' %>
        </div>
    </div>
    <div class="actions">
        <%= f.submit :class => 'btn btn-large btn-primary' %>
    </div>
<% end %>
